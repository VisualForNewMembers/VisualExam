#include<stdio.h>
int main()
{
 int a[11] = { 1,4,6,9,11,13,17,19,22,25 };
 int b, i, c;

 scanf_s("%d", &b);
 for (i = 0; i < 10; i++) {
  if (b >= a[i] && b <= a[i + 1]) {
   c = i + 1;
   break;
  }
 }
 for (i = 9; i >= c;i--) {
  a[i+1] = a[i];
}
 a[c] = b;
 for (i = 0; i < 11; i++) {
  printf("%-5d", a[i]);
 }



 return 0;
}


#include<stdio.h>
int main()
{
 int A[3][3] = { {1,2,3},{4,5,6},{7,8,9} };
 int B[3][2] = { {6,5},{4,3},{2,1} };
 int x,y,z;
 int C[3][2] = { 0 };
 for (x = 0; x < 3; x++) 
  for (y = 0; y < 2; y++) 
   
   for (z = 0; z < 3; z++)
    C[x][y] += A[x][z] * B[z][y];
   for (y = 0; y < 3; y++) {
    for (x = 0; x < 3; x++)
     printf("%-6d", C[x][y]);
   
  
 }
return 0;
}#include<stdio.h>
int main()
{
 int A[3][3] = { {1,2,3},{4,5,6},{7,8,9} };
 int B[3][2] = { {6,5},{4,3},{2,1} };
 int x,y,z;
 int C[3][2] = { 0 };
 for (x = 0; x < 3; x++) 
  for (y = 0; y < 2; y++) 
   
   for (z = 0; z < 3; z++)
    C[x][y] += A[x][z] * B[z][y];
   for (y = 0; y < 3; y++) {
    for (x = 0; x < 3; x++)
     printf("%-6d", C[x][y]);
   
  
 }
return 0;
}

#include<stdio.h>
int main()
{
 int a1, a2, a3, a4 ;
 int b1, b2, b3, b4 ;
 scanf_s("%d %d %d %d", &a1, &a2, &a3, &a4);
 scanf_s("%d %d %d %d", &b1, &b2, &b3, &b4);

 int A[2][2] = { {a1,a2},{a3,a4} };
 int B[2][2] = { {b1,b2}, {b3,b4} };
 int C[2] = { 0};
 
 
 C[0] = a1 * b3 - a2 * b4;
    C[1] = a3 * b1 - a4 * b2;

 printf("%d\n", C[0]);
 printf("%d\n", C[1]);

 

        


  
return 0;
}
